{
  "name": "craft-ai-kit-energy",
  "version": "0.1.0-alpha.5",
  "description": "craft ai energy prediction kit",
  "author": {
    "name": "craft ai",
    "email": "contact@craft.ai",
    "url": "http://craft.ai/"
  },
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/craft-ai/craft-ai-kit-energy/issues",
  "repository": "github:craft-ai/craft-ai-kit-energy",
  "homepage": "https://github.com/craft-ai/craft-ai-kit-energy#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run test:coverage",
    "lint": "npx eslint . --ignore-path .gitignore",
    "build": "npx babel src --out-dir lib",
    "lint:fix": "npm run lint -- --fix",
    "test:coverage": "npx nyc --reporter=html --reporter=text --reporter=text-summary npm run test:unit --",
    "test:unit": "npx ava",
    "test:update": "npm run test:coverage -- --update-snapshots",
    "test:watch": "npm run test:unit -- --watch"
  },
  "engines": {
    "node": ">=6.9.0 <7.0.0 || >=8.9.0"
  },
  "dependencies": {
    "craft-ai": "1.14.0",
    "date-easter": "0.2.2",
    "debug": "3.1.0",
    "fast-xml-parser": "3.3.9",
    "luxon": "1.1.0",
    "mem": "3.0.0",
    "most": "1.7.3",
    "most-buffer": "0.0.11",
    "most-nth": "0.0.11",
    "node-fetch": "2.1.2",
    "p-retry": "1.0.0",
    "quick-lru": "1.1.0",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/plugin-proposal-object-rest-spread": "latest",
    "@babel/polyfill": "latest",
    "@babel/preset-env": "latest",
    "@babel/register": "latest",
    "ava": "next",
    "dotenv": "latest",
    "eslint": "latest",
    "eslint-config-craft-ai": "^2.3.0",
    "nyc": "latest",
    "seedrandom": "latest"
  },
  "files": [
    "lib"
  ],
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "@babel/register"
    ],
    "files": [
      "test/**/*.test.js"
    ],
    "sources": [
      "src/**/*.js"
    ]
  }
}
